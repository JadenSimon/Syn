type Box<T> = {
  get: () => T
  set: (value: T) => void
}

declare function box<T>(value: T): Box<T>

export const bn1 = box({ prop: 0 })
export const bn2: Box<{ prop: number }> = box({ prop: 0 })

export const bb1 = box({ prop: false });
export const bb2: Box<{ prop: boolean }> = box({ prop: false });

