interface A {
    kind: 1
    data: string
}
interface B {
    kind: 2
    data: boolean
}
interface C {
    kind: 3
    data: number
}
interface D {
    kind: 4
    extra: number
}
type U = A | B | C | D
export function refine(input: U) {
    if (input.kind == 1) {
        return input
    }
    if (input.kind == 2) {
        return input
    }
    if (input.kind == 3) {
        return input
    }
}
export function refine2(input: U) {
    if (input.kind == 1) {
        return input
    }
    else if (input.kind == 2) {
        return input
    }
    else if (input.kind == 3) {
        return input
    }
    if (input.extra == 0) return input.extra
}